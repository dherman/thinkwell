{
  "name": "@thinkwell/acp",
  "version": "0.5.0-alpha.4",
  "description": "TypeScript implementation of ACP (Agent Client Protocol) extensions for Thinkwell",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "node --test --import tsx src/**/*.test.ts"
  },
  "keywords": [
    "acp",
    "mcp",
    "thinkwell",
    "agent",
    "llm"
  ],
  "author": "David Herman",
  "license": "MIT",
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.4.0",
    "@thinkwell/conductor": "workspace:*",
    "@thinkwell/protocol": "workspace:*",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
